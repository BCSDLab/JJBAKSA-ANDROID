<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SmallEditText">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">58dp</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLength">1</item>
        <item name="android:textColor">@color/color_000000</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@drawable/shape_rect_eeeeee_solid_radius_8</item>
        <item name="android:inputType">number</item>
    </style>

    <style name="FindIdCustomDialogStyle">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>

    <style name="ShapeAppearance.App.SmallComponent" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">37dp</item>
    </style>
    <style name="Widget.App.Button" parent="Widget.MaterialComponents.Button">
        <item name="shapeAppearance">@style/ShapeAppearance.App.SmallComponent</item>
    </style>
    <style name="OrangeButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/shape_rect_ff7f23_solid_radius_100</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SocialLoginAppBarStyle">
        <item name="android:background">@color/white</item>
        <item name="titleTextColor">@color/black</item>
        <item name="subtitleTextColor">@color/material_on_primary_emphasis_medium</item>
    </style>
    <style name="CircleCheckButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/chk_circ_radio_ff7f23</item>
        <item name="android:button">@null</item>
    </style>
    <style name="CircleToggleButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
        <item name="android:background">@drawable/chk_circ_radio_ff7f23</item>
    </style>
    <style name="RadiusButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">60dp</item>
        <item name="android:layout_marginEnd">60dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/bg_radius_100_button</item>
    </style>
    <style name="Regular">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_222222</item>
    </style>
    <style name="Regular.18">
        <item name="android:layout_width">wrap_content</item>
